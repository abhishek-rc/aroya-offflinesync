{
  "kind": "collectionType",
  "collectionName": "docs",
  "info": {
    "singularName": "doc",
    "pluralName": "docs",
    "displayName": "Documents"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "page_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "page_slug": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "components": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "shared.image-and-text-section",
        "shared.rich-text",
        "shared.table-section"
      ]
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "site_settings": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::site-setting.site-setting",
      "inversedBy": "documents"
    }
  }
}
